// Factorial function example

// Function to calculate factorial recursively
int factorial(int n) {
    if (n <= 1) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}

// Main function
int main() {
    int result;
    
    // Call factorial function
    result = factorial(5);
    
    // Print result
    printf("Factorial of 5: %d", result);
    
    return 0;
}
